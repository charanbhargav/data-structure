#include <stdio.h>

int main() {
    int firstMatrix[2][2] = {{1, 2}, {3, 4}};
    int secondMatrix[2][2] = {{5, 6}, {7, 8}};
    int resultMatrix[2][2];

    for (int i = 0; i < 2; i++) {
        for (int j = 0; j < 2; j++) {
            resultMatrix[i][j] = firstMatrix[i][j] + secondMatrix[i][j];
        }
    }

    printf("Resultant Matrix:\n");
    for (int i = 0; i < 2; i++) {
        for (int j = 0; j < 2; j++) {
            printf("%d ", resultMatrix[i][j]);
        }
        printf("\n");
    }

    return 0;
}


